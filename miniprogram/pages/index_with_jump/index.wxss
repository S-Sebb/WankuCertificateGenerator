.container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 100vh;
  background-color: #ffffff;
}

.title {
  margin-bottom: 40rpx;
  font-size: 48rpx;
  font-weight: bold;
  color: #333;
}

.nav-button {
  width: 500rpx;
  height: 80rpx;
  margin-top: 20rpx;
  background-color: #1aad19;
  color: white;
  border: none;
  border-radius: 10rpx;
  font-size: 36rpx;
}
